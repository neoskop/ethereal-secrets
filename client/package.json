    "@types/mocha": "9.1.1",
    "@types/sjcl": "1.0.30",
    "@types/superagent": "4.1.15",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "concurrently": "7.2.1",
    "jsdom": "19.0.0",
    "jsdom-global": "3.0.2",
    "mocha": "10.0.0",
    "mock-local-storage": "1.1.23",
    "node-localstorage": "2.2.1",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  },
  "dependencies": {
    "bluebird": "3.7.2",
    "sjcl": "1.0.8",
    "superagent": "7.1.3"
