{
  "name": "@neoskop/ethereal-secrets-client",
  "version": "1.2.3",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Arne Diekmann <diekmann@neoskop.de>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "tsc": "tsc -p .",
    "build": "npm run clean; npm run tsc",
    "start": "mocha -w --inspect=0.0.0.0:9229 --require ts-node/register src/test/*.ts",
    "test": "nyc --reporter=text mocha --require ts-node/register src/test/*.ts"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/chai-as-promised": "7.1.2",
    "@types/mocha": "7.0.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "concurrently": "5.1.0",
    "mocha": "7.1.1",
    "node-localstorage": "2.1.6",
    "nyc": "15.0.1",
    "rimraf": "3.0.2",
    "ts-node": "8.8.2",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@types/bluebird": "3.5.30",
    "@types/sjcl": "1.0.29",
    "@types/superagent": "4.1.7",
    "bluebird": "3.7.2",
    "sjcl": "1.0.8",
    "superagent": "5.2.2"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "src/test/*.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "engines": {
    "node": ">= 12.4.0"
  }
}
